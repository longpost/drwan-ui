{
  "name": "@drwan/ui",
  "version": "0.1.1",
  "description": "Shared UI components for DRWAN subdomains",
  "license": "MIT",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./theme.css": "./dist/theme.css"
  },
  "files": [
    "dist"
  ],
  "sideEffects": [
    "*.css"
  ],
  "peerDependencies": {
    "next": ">=13",
    "react": ">=18",
    "react-dom": ">=18"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.4.0"
  },
  "scripts": {
    "build": "tsc -p tsconfig.json && node ./scripts/copy-css.mjs",
    "prepare": "npm run build"
  }
}

